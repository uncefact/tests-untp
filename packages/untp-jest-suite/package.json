{
  "name": "untp-test-suite-mocha",
  "version": "0.1.0",
  "description": "A reusable Mocha-based testing library for UNTP (United Nations Transparency Protocol) credentials",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "untp-test": "dist/bin/untp-test.js"
  },
  "files": [
    "dist/**/*",
    "example-credentials/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "untp",
    "verifiable-credentials",
    "testing",
    "mocha",
    "w3c",
    "json-ld",
    "transparency"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "mocha": "^10.2.0",
    "@types/mocha": "^10.0.3",
    "chai": "^4.3.10",
    "@types/chai": "^4.3.6",
    "commander": "^11.1.0",
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "@types/node": "^20.8.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
