{
  "credentialType": "{{type}}",
  "version": "{{version}}",
  "path": "{{dataPath}}",
  "result": "WARN",
  "warnings": [
    {{#each warnings}}
      {
        "fieldName": "{{params.additionalProperty}}",
        "message": "This schema {{message}}"
      }
      {{#unless @last}},{{/unless}}
    {{/each}}
  ]
}