      "styles": {
        "primaryColor": "rgb(255, 207, 7)",
        "secondaryColor": "black",
        "tertiaryColor": "black"
      },
      "features": [
        {
          "name": "Upload VC",
          "components": [
            {
              "name": "RenderCheckList",
              "type": "EntryData",
              "props": {
                "requiredFields": ["steelID"],
                "checkBoxLabel": "Imported valid json:",
                "nestedComponents": [
                  {
                    "name": "ImportButton",
                    "type": "EntryData",
                    "props": {
