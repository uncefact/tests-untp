            {
              "name": "JsonForm",
              "type": "EntryData",
              "props": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "product": {
                      "type": "object",
                      "properties": {
                        "modelName": {
                          "type": "string"
                        },
                        "identifier": {
                          "type": "string",
