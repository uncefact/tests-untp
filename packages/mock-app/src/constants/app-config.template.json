            },
            {
              "name": "CustomButton",
              "type": "Submit",
              "props": {
                "variant": "contained",
                "style": { "backgroundColor": "rgb(255, 207, 7)", "color": "black", "display": "block", "margin": "0 auto", "marginTop": "20px" }
              }
            }
