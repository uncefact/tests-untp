                          "items": {
                            "type": "object",
                            "properties": {
                              "EventReference": {
                                "type": "string"
                              },
                              "EventType": {
                                "type": "string"
                              }
                            }
                          }
                        }
                      }
                    },
                    "issuedBy": {
                      "type": "object",
                      "properties": {
                        "name": {
                          "type": "string"
                        },
                        "identifiers": {
                          "type": "array",
                          "items": {
                            "type": "object",
                            "properties": {
                              "identifierURI": {
                                "type": "string"
                              },
                              "identiferValue": {
                                "type": "string"
                              }
                            }
                          }
                        }
                      }
                    },
                    "sustainabilityScore": {
                      "type": "integer",
