                    "dlrVerificationPage": "${DLR_VERIFICATION_PAGE}"
                  },
                  "dlr": {
                    "dlrAPIUrl": "${DLR_API_URL}",
                    "dlrAPIKey": "${DLR_API_KEY}"
                  },
                  "storage": {
                    "url": "${STORAGE_API_URL}",
                    "params": {
                      "bucket": "${STORAGE_BUCKET}",
                      "resultPath": "${RESULT_PATH_URI}"
                    },
                    "options": {
